 *
 * You're free to add application-wide styles to this file and they'll appear at the top of the
 * compiled file, but it's generally better to create a new file per style scope.
 *
 *= require_self
 *= require bootstrap.min
 *= require prettify/prettify
 *= require jquery.ui.all
 *= require timelineJS/timeline
 *= require_tree .
 */

 @font-face {
  font-family: 'Glyphicons Halflings';
  src: url('<%= asset_path('glyphicons.eot'                            ) %>');
  src: url('<%= asset_path('glyphicons.eot?#iefix'                     ) %>') format('embedded-opentype'), 
       url('<%= asset_path('glyphicons.woff'                           ) %>') format('woff'), 
       url('<%= asset_path('glyphicons.ttf'                            ) %>') format('truetype'), 
       url('<%= asset_path('glyphicons.svg#glyphicons_halflingsregular') %>') format('svg');
}